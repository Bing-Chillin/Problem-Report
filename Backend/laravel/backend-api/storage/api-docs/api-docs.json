{
    "openapi": "3.0.0",
    "info": {
        "title": "Report API",
        "description": "API for managing reports",
        "version": "1.0"
    },
    "paths": {
        "/api/reports": {
            "get": {
                "tags": [
                    "Reports"
                ],
                "summary": "Get a list of all reports",
                "operationId": "42398321972bc3074eef76718e0fd9dc",
                "responses": {
                    "200": {
                        "description": "List of reports returned successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Report"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "summary": "Create new report",
                "operationId": "318de4ef8123b483320dc9dbb45b6a43",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "subsystem": {
                                        "type": "string"
                                    },
                                    "text": {
                                        "type": "string"
                                    },
                                    "status": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    }
                }
            }
        },
        "/api/reports/{id}": {
            "put": {
                "summary": "Update the specified report",
                "operationId": "53aec2092fb1277fa3d06d65d59fa307",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of report to update",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "subsystem": {
                                        "type": "string"
                                    },
                                    "text": {
                                        "type": "string"
                                    },
                                    "status": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Report updated successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Report"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Report not found"
                    }
                }
            },
            "delete": {
                "summary": "Delete the specified report",
                "operationId": "a577f2ba73f34e6564524e2163d9bcdf",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of report to delete",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Number of deleted records",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "integer"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Report not found"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Report": {
                "title": "Report",
                "description": "Report model",
                "required": [
                    "subsystem",
                    "text",
                    "status",
                    "date",
                    "creator_id"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "subsystem": {
                        "type": "string",
                        "example": "Térkép"
                    },
                    "text": {
                        "type": "string",
                        "example": "Probléma a térképpel"
                    },
                    "status": {
                        "type": "string",
                        "example": "nyitott"
                    },
                    "date": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-07-31T12:00:00Z"
                    },
                    "creator_id": {
                        "type": "integer",
                        "example": 3
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-07-31T12:00:00Z"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-07-31T12:05:00Z"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
        {
            "name": "Reports",
            "description": "Reports"
        }
    ]
}